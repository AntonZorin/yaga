<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_orange_dark</item>
        <item name="colorPrimaryDark">@android:color/holo_orange_light</item>
        <item name="colorAccent">@android:color/holo_red_dark</item>
    </style>

    <style name="Tv22Black" parent="TextAppearance.AppCompat">
        <item name="color">@color/colorBlack</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="Tv16Black" parent="TextAppearance.AppCompat">
        <item name="color">@color/colorBlack</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>